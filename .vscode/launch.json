    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/sha256_challenge",
            "args": [ 
                    "-C",
                    "BRHw1Yy01Yi0zNS0yZi00Zi1iNHxkZXZlbG9wZXJ8qrvM3e7/qqusra6vuru8vQ==",
                    "-G",
                    //"0a-26-6d-9e-41-30", 
                    "5c-5b-35-2f-4f-b4", 
                    "-F",
                    "~/codigo/mist-ap41/c2/e.bin",//,
                    //"-K",
                    //"deadbeefdeadbeefdeadbeefdeadbeef",
                    "-R",
                    "aabbccddeeffaaabacadaeafbabbbcbd",
                    "-i"
                ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}